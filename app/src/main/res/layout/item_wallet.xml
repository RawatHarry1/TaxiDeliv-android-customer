<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:app="http://schemas.android.com/apk/res-auto">
    <androidx.appcompat.widget.LinearLayoutCompat
        xmlns:android="http://schemas.android.com/apk/res/android"
        android:layout_width="match_parent"
        android:layout_marginTop="5dp"
        android:layout_marginBottom="10dp"
        android:orientation="vertical"
        android:background="@drawable/bg_white_8_rad_1_stroke"
        android:layout_height="wrap_content">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            >

            <androidx.appcompat.widget.AppCompatTextView
                android:id="@+id/titleAmountAdded"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@color/black_text_color2"
                app:fontFamily="@font/poppins_medium"
                android:textSize="12sp"
                android:layout_alignParentStart="true"
                android:text="@string/amount_added"
                android:layout_marginStart="16dp"
                android:layout_marginTop="6dp"
                />


            <androidx.appcompat.widget.AppCompatTextView
                android:id="@+id/tvAmountAdded"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@color/green_text_color"
                app:fontFamily="@font/poppins_medium"
                android:textSize="12sp"
                android:layout_alignParentEnd="true"
                android:text="@string/_20_00"
                android:layout_marginEnd="16dp"
                android:layout_marginTop="6dp"
                />





        </RelativeLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="@color/stroke_light"
            android:layout_marginTop="6dp"
            />


        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            >

            <androidx.appcompat.widget.AppCompatTextView
                android:id="@+id/titleDate"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@color/black_text_color2"
                app:fontFamily="@font/poppins_regular"
                android:textSize="12sp"
                android:layout_alignParentStart="true"
                android:text="@string/date"
                android:layout_marginStart="16dp"
                android:layout_marginTop="6dp"
                />


            <androidx.appcompat.widget.AppCompatTextView
                android:id="@+id/tvDate"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@color/black_text_color2"
                app:fontFamily="@font/poppins_regular"
                android:textSize="12sp"
                android:layout_alignParentEnd="true"
                android:text="@string/_10_00_am"
                android:layout_marginEnd="16dp"
                android:layout_marginTop="6dp"
                />





        </RelativeLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            >

            <androidx.appcompat.widget.AppCompatTextView
                android:id="@+id/titleTime"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@color/black_text_color2"
                app:fontFamily="@font/poppins_regular"
                android:textSize="12sp"
                android:layout_alignParentStart="true"
                android:text="@string/time"
                android:layout_marginStart="16dp"
                android:layout_marginTop="9dp"
                />


            <androidx.appcompat.widget.AppCompatTextView
                android:id="@+id/tvTime"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@color/black_text_color2"
                app:fontFamily="@font/poppins_regular"
                android:textSize="12sp"
                android:layout_alignParentEnd="true"
                android:text="@string/jan_12_2020"
                android:layout_marginEnd="16dp"
                android:layout_marginTop="9dp"
                />





        </RelativeLayout>



        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="9dp"
            >

            <androidx.appcompat.widget.AppCompatTextView
                android:id="@+id/titleProcessNumber"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@color/black_text_color2"
                app:fontFamily="@font/poppins_regular"
                android:textSize="12sp"
                android:layout_alignParentStart="true"
                android:text="@string/engagement_id"
                android:layout_marginStart="16dp"
                android:layout_marginTop="9dp"
                />


            <androidx.appcompat.widget.AppCompatTextView
                android:id="@+id/tvProcessNo"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@color/black_text_color2"
                app:fontFamily="@font/poppins_regular"
                android:textSize="12sp"
                android:layout_alignParentEnd="true"
                android:text="@string/_301"
                android:layout_marginEnd="16dp"
                android:layout_marginTop="9dp"
                />





        </RelativeLayout>





    </androidx.appcompat.widget.LinearLayoutCompat>
</layout>